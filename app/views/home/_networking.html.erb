
 <div class='row'>

  <div class='span0 well text-center'>

 <% if meetups.blank? %>
    <p>No meetups meet your criteria<p>
      <% else %>


<table id="locations" class ="table1" cellspacing="0" width="100%">
  <thead>
  
  <tr>
    <th><%= "Meetup events based on your interest" %></th>
    <th>Meetup Group Name</th>
    <th>Event Name</th>
    <th>Status</th>
  </tr>
  </thead>

<body>
  <tr>
      <% meetups["results"].each do |meetup| %>
        
          <td><%= meetup["group"]["name"] %></td>
        
           <td><%= link_to meetup["name"], meetup["event_url"], target: "_blank" %></td>
           <td><%= meetup["status"] %></td>
  </tr>
      
      <% end %>
    
  </body>
</table>
<% end %>

